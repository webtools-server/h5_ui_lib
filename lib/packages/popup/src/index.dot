<div class="{{=it.classes.wrap}}">
  <div class="ui-overlay {{=it.classes.mask}}"></div>
  <div class="ui-popup">
    <div class="ui-popup__title {{=it.classes.title}}">{{?it.title}} {{=it.title}} {{?}}
      {{?it.showClose}}
        <div et="click:close" class="ui-popup__fn-button"><i class="{{=it.classes.close}}"></i></div>
      {{?}}
    </div>
    <div class="ui-popup__content {{=it.classes.content}}">{{=it.content}}</div>
    {{?it.footer}}
      <div class="ui-popup__footer {{=it.classes.footer}}">{{=it.footer}}</div>
    {{?}}
  </div>
</div>
